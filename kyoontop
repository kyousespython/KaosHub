    local a = Window:MakeTab({
        Name = "Locks",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })


    a:AddToggle({
        Name = "Aimlock",
        Default = false,
        Callback = function(Value)
            getgenv().AimPart = "HumanoidRootPart"
